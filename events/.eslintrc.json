{
  "extends": "airbnb-base",
  "env": {
     "es6": true,
     "browser": true,
     "jest": true
  },
  "rules": {
    "no-restricted-syntax": [
      "error",
      "LabeledStatement",
      "WithStatement"
    ],
    "indent": ["error", 4],
    "linebreak-style": "off",
    "no-multiple-empty-lines": ["error", { "max": 1, "maxEOF": 0 }]
  }
}